{
  "input_dir": "test_pdfs",
  "index_dir": "test_index",
  "parser": {
    "extract_tables": false,
    "fallback_to_raw": true
  },
  "chunker": {
    "chunk_size": 1000,
    "chunk_overlap": 200
  },
  "embedder": {
    "model_name": "sentence-transformers/all-MiniLM-L6-v2",
    "batch_size": 64,
    "device": null,
    "normalize": true,
    "seed": 42
  },
  "retriever": {
    "top_k": 5
  },
  "indexing": {
    "checkpoint_interval": 50,
    "n_workers": 2,
    "pdf_timeout": 300,
    "enable_gc_per_batch": true,
    "log_memory_usage": false,
    "continue_on_error": true,
    "max_failures": -1
  },
  "llm": {
    "mode": "openai",
    "model_name": "gpt-3.5-turbo",
    "temperature": 0.0,
    "max_tokens": 1024,
    "api_base": null,
    "api_key": null
  }
}